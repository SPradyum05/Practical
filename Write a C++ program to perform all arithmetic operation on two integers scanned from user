#include <iostream>
using namespace std;

int main()
{
    int a, b;
    int c, d;
    int e, f;
    int g;
    
    cout<<"Enter First Value: ";
    cin>>a;
    
    cout<<"Enter Second Value: ";
    cin>>b;
    
    c=a+b;
    d=a-b;
    e=a*b;
    f=a/b;
    g=a%b;
    
    cout<<"addition="<<c<<endl;
    cout<<"subtraction="<<d<<endl;
    cout<<"multiplication="<<e<<endl;
    cout<<"division="<<f<<endl;
    cout<<"modulas="<<g<<endl;
    
    

    return 0;
}
